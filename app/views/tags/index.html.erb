<h1>Visible Tags</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Rssi</th>
      <th>Antenna</th>
      <th>Last seen at</th>
        <!--
      <th></th>
      <th></th>
      <th></th>
      -->
    </tr>
  </thead>

  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= tag.tag_id %></td>
        <td><%= tag.rssi %></td>
        <td><%= tag.antenna %></td>
        <td><%= tag.last_seen_at %></td>
        <!--
        <td><%= link_to 'Show', tag %></td>
        <td><%= link_to 'Edit', edit_tag_path(tag) %></td>
        <td><%= link_to 'Destroy', tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		-->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<p>Updated every 10 minutes</p>
